add_newline = true

[character]
#success_symbol = "[❯](white)[❯](cyan)[❯](bold green)" # prompt fit for kitty terminal
#error_symbol = "[❯](red)[❯](red)[❯](red)" # prompt fit for kitty terminal
success_symbol = "[](bold white)[](bold cyan)[](bold green)" # prompt fit for alacritty terminal
error_symbol = "[](bold red)[](bold red)[](bold red)"

[status]
disabled = false
format = "[$symbol $status]($style) "

